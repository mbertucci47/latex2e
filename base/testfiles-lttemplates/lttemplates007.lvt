\documentclass{minimal}
\input{regression-test}
\ExplSyntaxOn
\debug_on:n { check-declarations , deprecation , log-functions }
\ExplSyntaxOff

\begin{document}
\START
\AUTHOR{Joseph Wright}
\ExplSyntaxOn

\TEST { Erroneous~types }
  {
    \OMIT
    \NewTemplateType { foo } { 1 }
    \TIMO
    \NewTemplateType { foo } { 1 }
  }

\TEST { Erroneous~interfaces }
  {
    \OMIT
    \NewTemplateInterface { foo } { foo } { 1 } { }
    \TIMO
    \NewTemplateInterface { foo } { foo } { 1 } { }
  }

\TEST { Erroneous~code }
  {
    \OMIT
    \NewTemplateCode { foo } { foo } { 1 } { } { }
    \TIMO
    \NewTemplateCode { foo } { foo } { 1 } { } { }
  }
  
\TEST { Erroneous~ordering }
  {
    \NewTemplateCode { foo } { foobar } { 1 } { } { }
  }

\TEST { Erroneous~template~copy }
  {
    \OMIT
    \NewTemplateCopy { foo } { foobaz } { foo }
    \TIMO
    \NewTemplateCopy { foo } { foobaz } { foo }
  }

\TEST { Erroneous~instance~creation }
  {
    \OMIT
    \NewInstance { foo } { foo } { foo } { }
    \TIMO
    \NewInstance { foo } { foo } { foo } { }
  }

\TEST { Erroneous~instance~copy }
  {
    \OMIT
    \NewInstance { foo } { foo } { foo } { }
    \NewInstanceCopy { foo } { foobar } { foo }
    \TIMO
    \NewInstanceCopy { foo } { foobar } { foo }
  }

\END